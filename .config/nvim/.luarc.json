{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "runtime": {
    "version": "LuaJIT",
    "path": [
      "?.lua",
      "?/init.lua"
    ],
    "pathStrict": true
  },
  "workspace": {
    "library": [
      "$VIMRUNTIME",
      "$VIMRUNTIME/lua",
      "${3rd}/luv/library",
      "$HOME/.local/share",
      "lua/utils",
      "$HOME/.local/share/nvim/site/pack/packer/start/nvim-treesitter/lua"
    ],
    "checkThirdParty": false
  }
}
